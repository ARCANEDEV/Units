{
    "name":        "arcanedev/units",
    "description": "Units package that allows you to manage multiple units of measure in easy way",
    "keywords":    ["arcanedev", "laravel", "units"],
    "homepage":    "https://github.com/ARCANEDEV/Units",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               "^7.0.0",
        "arcanedev/support": "~4.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5.0",
        "phpunit/phpcov":      "~4.0",
        "phpunit/phpunit":     "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Units\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Units\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.0"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\Units\\UnitsServiceProvider"
            ]
        }
    }
}
